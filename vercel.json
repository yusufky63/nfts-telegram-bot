{
  "version": 2,
  "functions": {
    "api/webhook.js": {
      "maxDuration": 10
    },
    "api/index.js": {
      "maxDuration": 5
    }
  },
  "routes": [
    {
      "src": "/api/webhook",
      "dest": "/api/webhook.js"
    },
    {
      "src": "/",
      "dest": "/api/index.js"
    }
  ],
  "crons": [
    {
      "path": "/api/cron",
      "schedule": "*/5 * * * *"
    }
  ]
} 